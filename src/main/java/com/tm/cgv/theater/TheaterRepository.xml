<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tm.cgv.theater.TheaterRepository">
	<select id="cinemaSelect" resultType="CinemaVO">
		select * from cinema; 
	</select>
	
	<insert id="theaterInsert" parameterType="TheaterVO">
		insert into theater values #{cinemaNum},#{name},#{seatCount},#{filmType}
	</insert>
</mapper>