<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tm.cgv.cuponInfo.CuponInfoRepository">

	<select id="cuponInfoSelectOne" parameterType="CuponInfoVO" resultType="CuponInfoVO">
		select * from cuponInfo where serialNum = #{serialNum} and pwd=#{pwd}
	</select>
	
	<delete id="cuponInfoDelete" parameterType="CuponInfoVO">
		delete from cuponInfo where num=#{num}
	</delete>
	
	<select id="cuponInfoSelect" parameterType="CuponInfoVO" resultType="CuponInfoVO">
		select * from cuponInfo where num=#{num}
	</select>

</mapper>