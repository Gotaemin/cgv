<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tm.cgv.payment.PaymentRepository">

	<insert id="paymentInsert" parameterType="PaymentVO" useGeneratedKeys="true" keyProperty="num">
		insert into payment values(#{num},#{imp_uid},#{merchant_uid},#{pg},#{pay_method},#{apply_num})
	</insert>
	

</mapper>