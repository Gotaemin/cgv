<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tm.cgv.movieVideo.MovieVideoRepository">

	<insert id="movieVideoInsert" parameterType="MovieVideoVO" useGeneratedKeys="true" keyProperty="num">
		insert into movieVideo(num,movieNum,videolink) values(#{num},#{movieNum},#{videolink})
	</insert>
	
	<update id="movieVideoUpdate" parameterType="MovieVideoVO">
		update movieVideo set videolink=#{videolink} where movieNum=#{movieNum};		
	</update>
	
	





</mapper>