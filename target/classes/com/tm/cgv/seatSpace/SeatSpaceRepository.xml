<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tm.cgv.seatSpace.SeatSpaceRepository">
	<insert id="seatSpaceInsert" parameterType="SeatSpaceVO">
		insert into seatSpace values (0, #{theaterNum}, #{rowOrCol}, #{index})
	</insert>


	<select id="seatSpaceSelect" parameterType="SeatSpaceVO" resultType="SeatSpaceVO">
		select * from seatSpace where theaterNum = #{theaterNum};
	</select>
 

</mapper>